# Container registry (full path including image name)
REGISTRY=registry.example.com/ghostpipe-excalidraw

# Domain for the Excalidraw instance
DOMAIN=excalidraw.example.com

# Docker image version (optional, defaults to latest)
VERSION=latest

# Docker context for deployment (default: default)
DOCKER_CONTEXT=default

# Optional: Image name for local builds
IMAGE_NAME=ghostpipe-excalidraw